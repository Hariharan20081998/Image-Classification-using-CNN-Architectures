 = sum_sqr_image[:,:,mid:-mid,mid:-mid].sqrt()\n","    per_img_mean = s_deviation.mean()\n","    \n","    ## Divisive Normalization\n","    divisor = np.maximum(per_img_mean.numpy(),s_deviation.numpy())\n","    divisor = np.maximum(divisor, 1e-4)\n","    new_image = centered_image / torch.Tensor(divisor)\n","    return new_image"],"execution_count":13,"outputs":[]},{"cell_type":"code","metadata":{"id":"oacjtqf0CfVV","colab_type":"code","colab":{"base_uri":"https://localhost:8080/","height":552},"executionInfo":{"status":"ok","timestamp":1595865293444,"user_tz":-330,"elapsed":933,"user":{"displayName":"Hariharan S","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14Gj54JJCqGON5hm1gltkqJNTL5-YfKs7hwrqeQZJPQ=s64","userId":"10376317834216671317"}},"outputId":"89f7d17a-f95a-46cf-df51-52819db76139"},"source":["l=img_arr[20]\n","image_tensor = torch.Tensor([np.array(np.reshape(l,(32,32,3))).transpose((2,0,1))])\n","ret=LocalContrastNorm(image_tensor,3)\n","ret = ret[0].numpy().transpose((1,2,0))\n","scaled_ret = (ret - ret.min())/(ret.max() - ret.min())\n","display(l)\n","plt.imshow(ret)\n","plt.show()"],"execution_count":16,"outputs":[{"output_type":"display_data","data":{"image/png":"\n","text/plain":["<Figure size 432x288 with 1 Axes>"]},"metadata":{"tags":[],"needs_background":"light"}},{"output_type":"stream","text":["Clipping input data to the valid range for imshow with RGB data ([0..1] for floats or [0..255] for integers).\n"],"name":"stderr"},{"output_type":"display_data","data":{"image/png":"\n","text/plain":["<Figure size 432x288 with 1 Axes>"]},"metadata":{"tags":[],"needs_background":"light"}}]},{"cell_type":"code","metadata":{"id":"co6wAFUwz6Xj","colab_type":"code","colab":{"base_uri":"https://localhost:8080/","height":266},"executionInfo":{"status":"ok","timestamp":1595865315774,"user_tz":-330,"elapsed":3035,"user":{"displayName":"Hariharan S","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14Gj54JJCqGON5hm1gltkqJNTL5-YfKs7hwrqeQZJPQ=s64","userId":"10